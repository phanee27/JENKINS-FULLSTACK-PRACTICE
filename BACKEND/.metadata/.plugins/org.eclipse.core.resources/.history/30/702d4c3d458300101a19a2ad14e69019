package com.klef.library.service;

import java.util.Optional;

import com.klef.library.entity.libraryEntity;
import com.sun.tools.javac.util.List;


public interface libraryService {
	libraryEntity addBook(libraryEntity book);
	List<libraryEntity> getAllBooks();
    Optional<libraryEntity> getBookById(int id);
    libraryEntity updateBook(libraryEntity book);
    void deleteBook(int id);
}
